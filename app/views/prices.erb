<div class="price-title"><h4><%= @car.year %> <%= @car.make %></h4>
	<h4><%= @car.model %></h4>
	<a href="/cars/<%= @id %>/prices/new">Add your own price</a>
	<a href="/cars/<%= @id %>/prices/edit">Edit your submitted prices</a>
</div>
<div class="price-list">
	
	<% @car.prices.each do |price| %>
	<ul>
		<li>$<%= price.cost %></li>
	</ul>
	<% end %>
</div>
