<div class="center-frame">
	<H4>Cars Makes</H4>
	<ul class="make"><% Car.all.group_by(&:make).map do |make, value| %>
		<li class="make-li"><%= make %></li>
		<ul class="models">
			<% value.map do |car| %>
			<li><a href="/cars/<%= car.id %>/prices"><%= car.model %></a></li>
			<% end %>
		</ul>
		<% end %>
	</ul>
</div>
